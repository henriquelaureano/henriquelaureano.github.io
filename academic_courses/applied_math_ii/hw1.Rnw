\section*{Problem 6} \addcontentsline{toc}{section}{Problem 6}

\horrule{1pt} \\

\textbf{Find all the solutions of equation \(z^{8} - 2z^{4} + 1 = 0\).} \\

\underline{Solution:}

\begin{align*}
 z^{8} - 2z^{4} + 1 = 0 \quad \Rightarrow \quad
 (z^{4} - 1)(z^{4} - 1) = 0 \quad \Rightarrow \quad
 z^{4} - 1 = 0 \quad \Rightarrow \quad
 (z^{2} - 1)(z^{2} + 1) = 0
\end{align*}

\begin{align*}
 z^{2} - 1 = 0 & \quad \Rightarrow \quad (z - 1)(z + 1) = 0
                 \quad \Rightarrow \quad z = \pm 1. \\
 z^{2} + 1 = 0 & \quad \Rightarrow \quad z^{2} = -1
                 \quad \Rightarrow \quad z = \pm \sqrt{-1}
                 \quad \Rightarrow \quad z = \pm i.
\end{align*}

\[ \boxed{{\rm Solutions}: \quad z = -1, \quad z = 1, \quad z = -i, \quad z = i.}
\]

\hfill \(\square\)

